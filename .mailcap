text/html;                  elinks -eval 'set document.codepage.force_assumed = 1' %s; needsterminal; description=HTML Text; nametemplate=%s.html
text/html;                  elinks -dump %s; nametemplate=%s.html; copiousoutput
text/plain;                 needsterminal; copiousoutput;
image/*;                    xv %s ; test=test -n "$DISPLAY";
image/*;					fbi %s; needsterminal;
application/pdf;            evince '%s'; test=test -n "$DISPLAY";
application/pdf;            fbgs -a -c %s; needsterminal;
application/msword;         soffice '%s'; test=test "$DISPLAY" != "" ; description="Doc file" ; nametemplate=%s.doc
application/vnd.ms-excel;   soffice '%s'; test=test "$DISPLAY" != "" ; description="XLS file" ; nametemplate=%s.xls
