<?xml version="1.0" encoding="utf-8"?>
<openbox_menu>
    <menu id="root-menu" label="OpenBox 3">
        <item label="Urxvt">
            <action name="Execute">
                <execute>
                    urxvtc
                </execute>
            </action>
        </item>
        <item label="Vim">
            <action name="Execute">
                <execute>
                    gvim
                </execute>
            </action>
        </item>
        <item label="Thunar">
            <action name="Execute">
                <execute>
                    thunar
                </execute>
            </action>
        </item>
        <separator/>
        <menu id="root-menu-852035" label="Internet">
            <item label="Firefox">
                <action name="Execute">
                    <execute>
                        firefox
                    </execute>
                </action>
            </item>
            <item label="Mutt">
                <action name="Execute">
                    <execute>
                        urxvtc -name mutt -geometry 100x45 -e mutt -y
                    </execute>
                </action>
            </item>
            <item label="Canto">
                <action name="Execute">
                    <execute>
                        urxvtc -geometry 100x45 -e canto -u
                    </execute>
                </action>
            </item>
            <item label="Empathy">
                <action name="Execute">
                    <execute>
                        empathy
                    </execute>
                </action>
            </item>
            <item label="Ekg2">
                <action name="Execute">
                    <execute>
                        urxvtc -geometry 60x30 -name ekg2 -e ~/bin/ekg2-go
                    </execute>
                </action>
            </item>
            <item label="rTorrent">
                <action name="Execute">
                    <execute>
                        urxvtc -e ~/bin/rtorrent-go
                    </execute>
                </action>
            </item>
            <item label="FileZilla">
                <action name="Execute">
                    <execute>
                        filezilla
                    </execute>
                </action>
            </item>
        </menu>
        <menu id="root-menu-512810" label="Grafika">
            <item label="Gimp">
                <action name="Execute">
                    <execute>
                        gimp
                    </execute>
                </action>
            </item>
            <item label="EoG">
                <action name="Execute">
                    <execute>
                        eog
                    </execute>
                </action>
            </item>
        </menu>
        <menu id="root-menu-512820" label="Multimedia">
            <item label="Ncmpcpp">
                <action name="Execute">
                    <execute>
                        urxvtc -geometry 90x10 -name ncmpcpp -e ncmpcpp 
                    </execute>
                </action>
            </item>
            <item label="Sonata">
                <action name="Execute">
                    <execute>
                        sonata
                    </execute>
                </action>
            </item>
            <item label="Smplayer">
                <action name="Execute">
                    <execute>
                        smplayer
                    </execute>
                </action>
            </item>
        </menu>
        <menu id="root-menu-243111" label="Biuro">
            <item label="Writer">
                <action name="Execute">
                    <execute>
                        soffice -writer
                    </execute>
                </action>
            </item>
            <item label="Impress">
                <action name="Execute">
                    <execute>
                        soffice -impress
                    </execute>
                </action>
            </item>
            <item label="Calc">
                <action name="Execute">
                    <execute>
                        soffice -calc
                    </execute>
                </action>
            </item>
            <item label="Sunbird">
                <action name="Execute">
                    <execute>
                        sunbird
                    </execute>
                </action>
            </item>
            <item label="Evince">
                <action name="Execute">
                    <execute>
                        evince
                    </execute>
                </action>
            </item>
        </menu>
        <separator/>
        <menu id="40" label="OpenBox">
            <menu id="client-list-menu"/>
            <item label="Reconfigure">
                <action name="Reconfigure"/>
            </item>
            <separator/>
            <item label="Exit">
                <action name="Exit"/>
            </item>
        </menu>
        <separator/>
        <menu id="50" label="Exitus">
            <item label="Zablokuj">
                <action name="Execute">
                    <execute>
                        gnome-screensaver-command -l
                    </execute>
                </action>
            </item>
            <item label="Przełącz">
                <action name="Execute">
                    <execute>
                        gnome-screensaver-command -l
                    </execute>
                </action>
                <action name="Execute">
                    <execute>
                        gdm-control --switch-user
                    </execute>
                </action>
            </item>
            <item label="Wyloguj">
                <action name="Exit"/>
            </item>
            <separator/>
            <item label="Restart">
                <action name="Execute">
                    <execute>
                        gdm-control --reboot
                    </execute>
                </action>
                <action name="Execute">
                    <execute>
                        openbox --exit
                    </execute>
                </action>
            </item>
            <item label="Wyłącz">
                <action name="Execute">
                    <execute>
                        gdm-control --shutdown
                    </execute>
                </action>
                <action name="Execute">
                    <execute>
                        openbox --exit
                    </execute>
                </action>
            </item>
        </menu>
    </menu>
</openbox_menu>
