[dom]
title = domowe
filterexpr = @dom
groupexpr = desc:project

[dziś]
title = dziś
filterexpr = due:tod -@praca
sortexpr = desc:importance,due,desc:priority
groupexpr = context

[zaległe]
title = zaległe
filterexpr = due:<tod -@praca
sortexpr = due,desc:importance,desc:priority
groupexpr = context

[bez kontekstu]
filterexpr = -@

[bez priorytetu]
filterexpr = (<Z)

[NoDate]
title = No Date
filterexpr = -due:
groupexpr = context
show_all = 1
